    "version": "0.2.0",
    "configurations": [

        {
            "name": "Chrome",
            "type": "chrome",
            "sourceMaps": true,
            "request": "launch",
            "url": "http://localhost:9000",
            "webRoot": "${workspaceRoot}/src",
            "sourceMapPathOverrides": {
              "webpack:///src/*": "${webRoot}/*"
            }
        }
    ]
}